{
    "name": "ps1",
    "dockerFile": "Dockerfile",
    "workspaceFolder": "/workspaces/ps1",
    "customizations": {
        "vscode": {
            "extensions": [
                "Grumpycoders.psx-dev",
                "ms-vscode.cpptools-extension-pack",
                "webfreak.debug"
            ]
        }
    },
    "containerEnv": {
        "DISPLAY": ":${localEnv:DISPLAY}"
    },
    "mounts": [
        "source=/tmp/.X11-unix,target=/tmp/.X11-unix,type=bind,consistency=cached"
    ]
}
